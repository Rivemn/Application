# Имя проекта для dev-окружения 
PROJECT_NAME=myapp_dev

# Установка окружения разработки
ASPNETCORE_ENVIRONMENT=Development
NODE_ENV=development
BUILD_CONFIGURATION=Debug

# Настройки базы данных для разработки
DB_NAME=myapp_db_dev
DB_USER=postgres
DB_PASSWORD=postgres
DB_PORT=5432

# Порт бэкенда (внутренний)
BACKEND_PORT=5000
# URL, который будет использовать ваш фронтенд
BACKEND_URL=http://localhost/api

# Уровень логирования
LOG_LEVEL=Debug

# JWT настройки
JWT__KEY=REPLACE_THIS_WITH_A_SIMPLE_DEV_KEY_NOT_FOR_PROD
JWT__ISSUER=myapp_backend
JWT__AUDIENCE=myapp_frontend
JWT__LIFETIME_MINUTES=1440

# Gemini API
AiAssistant__GeminiApiKey=YOURKEY

# Домен и порты NGINX
DOMAIN=localhost
NGINX_PORT=80
NGINX_PORT_SSL=443
